@using Leandro.LojaVirtual.Web.HtmlHelpers
@model  Leandro.LojaVirtual.Web.Models.ProdutosViewModel

@foreach (var p in Model.Produtos)
{
    @Html.Partial("ProdutoDetalhes", p);
}
<div class="btn-group pull-right ">
    @Html.PageLinks(Model.Paginacao, x => Url.Action("ListarPaginaProduto", new { pagina = x }))
</div>